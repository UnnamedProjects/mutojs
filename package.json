{
  "name": "@unnamed-projects/mutojs",
  "version": "0.0.4",
  "author": "Robert Baxter",
  "license": "MIT",
  "description": "Tool for dispatching actions from React Client Components to React Server Components",
  "files": [
    "dist/",
    "package.json"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react"
  },
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/UnnamedProjects/mutojs.git"
  },
  "keywords": [
    "react",
    "react server components",
    "next",
    "muto",
    "mutojs"
  ],
  "scripts": {
    "build": "esbuild src/index.ts src/react/index.ts --bundle --outdir=dist/ && cp package.json dist && tsc -p tsconfig.build.json",
    "prepublish": "pnpm build",
    "publish": "pnpm publish dist",
    "test": "jest --runInBand"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.28",
    "esbuild": "^0.17.11",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^27.5.1",
    "next": "^13.2.4",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.8.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "next": "^13.2.4",
    "react": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "dependencies": {
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-module-utils": "^2.7.4",
    "eslint-scope": "^7.1.1",
    "eslint-visitor-keys": "^3.3.0",
    "prettier-linter-helpers": "^1.0.0"
  }
}
